version 43
__lua__
--game config
function _init()
	--gets called when the prog runs
	--player variables
	playerx = 63
	playery = 63
	flap = 0
	flap_amount = 7
	gravity = 2
	
	player_spr = 2
	
	score = 0
	
	coll = false
	
	state = "start"
	
	--custom func
	initialize_pipes()
end

function _update()
	if state == "game" then
		update_game()
	elseif state == "game_over" then
		update_game_over()
	elseif state == "start" then
		update_start()
	end
end

function _draw()
	if state == "game" then
		draw_game()
	elseif state == "game_over" then
		draw_game_over()		
	elseif state == "start" then
		draw_start()
	end
end

function update_game()
	--gets called 30x/sec
	playery = playery + gravity --grav
	
	if btnp(âŽ) then
		flap = flap_amount
	end
	
	if flap > 0 then
			flap = flap - 1
			player_spr = 1
		else
			player_spr = 2
	end
	
	playery = playery - flap
	
	if playery >= 120 then
		playery = 120
	elseif playery < 0 then
		playery = 0
	end
	
	update_pipes()
	update_score()
	collide()
end

function draw_game()
	--gets called 30x/sec after upt
	cls()
	map()
	
	draw_pipes()
	
	spr(player_spr,playerx,playery)
	print("score: ".. score)
end

function update_game_over()
	if btnp(ðŸ…¾ï¸) then
		_init()
	end
end

function draw_game_over()
	cls()
	print("game over", 48, 48)
	print("highscore: "..score, 42, 58)
	print("press z to restart", 30, 68)
end

function update_start()
	if btnp(ðŸ…¾ï¸) then
		state = "game"
	end
end

function draw_start()
	cls()
	map()
	print("a flappy bird demake", 28, 48)
	print("by feelgxxd", 46, 58)
	print("press z to start", 36, 68)
end
-->8
-- obstacle config
pipe_gap = 28

-- helper to get a safe random pipe y
function reset_pipe_y()
    local top_margin = 8   -- keep some space at top
    local bot_margin = 8   -- keep some space at bottom

    -- bottom of the gap must stay within [min_y, max_y]
    local min_y = pipe_gap + top_margin
    local max_y = 128 - bot_margin

    return flr(rnd(max_y - min_y)) + min_y
end

function initialize_pipes()
    pipe1b_x = 128
    pipe1b_y = reset_pipe_y()
end

function update_pipes()
    -- move pipe left
    pipe1b_x -= 1
    
    -- recycle pipe when it goes off-screen
    if pipe1b_x < -16 then
        pipe1b_x = 128
        pipe1b_y = reset_pipe_y()
    end
end

function draw_pipes()
    -- draw bottom pipe
    spr(7, pipe1b_x, pipe1b_y, 2, 16)
    -- draw top pipe (shifted up by pipe height + gap)
    spr(7, pipe1b_x, pipe1b_y - 128 - pipe_gap, 2, 16)
end
-->8
--score / collisions
function update_score()
	if pipe1b_x == playerx then
		score += 1
	end
end

function collide()
    -- player hitbox
    local px = playerx
    local py = playery
    local pw = 6   -- bird width
    local ph = 6   -- bird height

    -- pipe dimensions (from your sprite)
    local pipe_w = 16  -- since you use spr(...,2,16), it's 2 tiles wide (8*2)
    local pipe_h = 128 -- full cave height

    -- bottom pipe hitbox
    local bot_x = pipe1b_x
    local bot_y = pipe1b_y
    local bot_w = pipe_w
    local bot_h = 128 - bot_y  -- bottom pipe goes down to ground

    -- top pipe hitbox
    local top_x = pipe1b_x
    local top_h = pipe1b_y - pipe_gap  -- full top pipe height
    local top_y = 0
    local top_w = pipe_w

    -- check bottom pipe collision
    local bottom_collides = not (
        px + pw < bot_x or
        px > bot_x + bot_w or
        py + ph < bot_y or
        py > bot_y + bot_h
    )

    -- check top pipe collision
    local top_collides = not (
        px + pw < top_x or
        px > top_x + top_w or
        py + ph < top_y or
        py > top_y + top_h
    )

    -- handle collision
    if bottom_collides or top_collides then
        coll = true
        state = "game_over"
    else
        coll = false
    end
end
